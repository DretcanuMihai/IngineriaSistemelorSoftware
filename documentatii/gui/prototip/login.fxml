<VBox alignment="CENTER" prefHeight="313.0" prefWidth="454.0" spacing="20.0" styleClass="linear-grad1" stylesheets="@style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bugmanager.controllers.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="5.0" />
    </padding>

    <Label text="Login">
      <font>
         <Font size="30.0" />
      </font></Label>
   <Label text="Username:">
      <font>
         <Font size="18.0" />
      </font>
   </Label>
   <TextField fx:id="usernameTextField" />
   <Label text="Password:">
      <font>
         <Font size="18.0" />
      </font>
   </Label>
   <PasswordField fx:id="passwordTextField" />
    <Button onAction="#onLoginButtonClick" styleClass="linear-grad2" text="Login" />
</VBox>
